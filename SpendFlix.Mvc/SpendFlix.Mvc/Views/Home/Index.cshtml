@model List<SpendFlix.Entities.UserDTO>
@{
    ViewData["Title"] = "Home Page";
}


@using (Html.BeginForm("Index", "Home", FormMethod.Post))
    {

        <div>

            <input type="submit" value="Kaydet" name="segment-text" class="Index" />
            <label></label>
        </div>
    }

    @for (int i = 0; i < Model.Count; i++)
    {
        <td>@Model[i].Email</td>
        <td>@Model[i].HashPassword</td>
        <td>@Model[i].FullName</td>
    }

